{
    "name": "@ashwamegh/vue-link-preview",
    "version": "3.3.0",
    "type": "module",
    "main": "dist/vuelinkpreview.umd.js",
    "module": "dist/vuelinkpreview.esm.js",
    "unpkg": "dist/vuelinkpreview.min.js",
    "files": [
        "dist/*"
    ],

    "exports": {
        ".": {
            "import": "./dist/vuelinkpreview.esm.js",
            "require": "./dist/vuelinkpreview.umd.js"
        }
    },
    "types": "./dist/types/index.d.ts",
    "scripts": {
        "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
        "preserve": "vite build",
        "serve": "vite preview --port 5050",
        "typecheck": "vue-tsc --noEmit",
        "preview": "vite preview",
        "test": "exit 0"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "vue": "^3.3.11",
        "@vitejs/plugin-vue": "^4.5.2",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-css-injected-by-js": "^3.3.1",
        "vue-tsc": "^1.8.26"
    },
    "author": "Shashank Shekhar",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ashwamegh/vue-link-preview/issues"
    },
    "homepage": "https://github.com/ashwamegh/vue-link-preview#readme",
    "repository": {
        "type": "git",
        "url": "git://github.com/ashwamegh/vue-link-preview.git"
    },
    "readme": "https://github.com/ashwamegh/vue-link-preview/blob/master/README.md",
    "funding": [
        {
            "type": "buymeacoffee",
            "url": "https://www.buymeacoffee.com/ashwameghwork"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/ashwamegh"
        }
    ]
}
